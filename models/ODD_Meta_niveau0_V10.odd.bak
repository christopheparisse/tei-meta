<?xml version="1.0" encoding="UTF-8"?>
    <?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
    <?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
    
    <TEI xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:svg="http://www.w3.org/2000/svg"
      xmlns:math="http://www.w3.org/1998/Math/MathML" xmlns="http://www.tei-c.org/ns/1.0">

	<teiHeader>
		<fileDesc> 
			<titleStmt>
			    <title >
               <idno   type="title">Personnalisation ODD Corpus Oraux et Multimodaux</idno>			  
               <bibl >			   
			   CORLI et IRCOM
               </bibl>
               <desc >
			  L'objectif de cette personnlaisation est de proposer un sous-ensemble d'éléments TEI pour décrire le niveau minimal des métadonnées de Corpus (niveau 0)
			   </desc>
               </title>
			   <principal >
				<name  type="org" >
				CORLI - IRCOM
				</name>
				<name  type="org" >
				Carole Etienne, Loic Liégeois ...
				</name>
            </principal>
			</titleStmt> 
			<publicationStmt>
				<publisher>CORLI wiki</publisher>
			</publicationStmt> 
			<sourceDesc> 
				<p>written from IRCOM recommandations</p>
			</sourceDesc>
		</fileDesc> 
	</teiHeader> 
 
  <text>
    <body>
 
	  <!-- definition de TEI_CORPO_METADATA-->
	  <schemaSpec ident="TEI_CORPO_METADATA" start="TEI"> <!-- ajouter teiCorpus par la suite si collections -->

		<!-- required modules -->
		<moduleRef key="tei"/>
			
		<!-- à compléter par : reg choice orig pour l'orthographe adapté -->
		<moduleRef key="core"
			include="title bibl desc name respStmt resp publisher pubPlace ref p media date "
			/>
				
		<moduleRef key="header"
			include="teiHeader fileDesc titleStmt idno principal publicationStmt availability licence sourceDesc profileDesc langUsage language 
					 encodingDesc appInfo application projectDesc classDecl taxonomy catDesc category" />
			
		<!-- à compléter par : body div text-->
		<moduleRef key="textstructure"  
			include="TEI text body div "
			/>
		   
		<!-- optional modules -->
		<!-- à compléter par : u annotationBlock incident kinesic pause shift vocal-->		
		<moduleRef key="spoken" 
			include="recordingStmt recording transcriptionDesc annotationBlock u vocal kinesic incident pause"
			/>
			
		<moduleRef key="namesdates"
			include="persName surname listPerson person age occupation langKnowledge langKnown listPlace place country settlement district "
			/>

		<moduleRef key="corpus"
			include="activity textDesc channel constitution derivation domain factuality interaction preparedness purpose particDesc settingDesc setting"
			/>
				
		<!-- à compléter par Christophe P -->
		<moduleRef key="linking" include="anchor seg timeline when"	/>			
		<moduleRef key="analysis" include=" w span spanGrp"/> <!-- ajouter interp interpGrp-->
		<moduleRef key="transcr"/> <!-- utilisé ???-->

	  	<!-- structure generale TEI -->
	  	<elementSpec ident="TEI" module="textstructure"  mode="change">
	  		<content>
	  			<one>
	  			<element name="teiHeader"  module="header"/>
	  			<element name="text" module="textstructure"/>
	  			</one>
	  		</content> 
	  	</elementSpec>
	  	
	  	<!-- structure generale teiHeader -->
	  	<elementSpec ident="teiHeader" module="header"  mode="change">
	  		<content>
	  			<sequence minOccurs="1" maxOccurs="1">
	  			<desc xml:lang="fr">Titre, diffusion et données primaires</desc>
	  			<desc xml:lang="en">Title, diffusion and primary data</desc>
	  			<element name="fileDesc"  module="header">
	  				<content>
	  					<one>
	  						<element name="titleStmt"  module="header"/>
	  						<element name="publicationStmt"  module="header"/>
	  						<element name="sourceDesc"  module="header"/>
	  					</one>
	  				</content> 
	  			</element>

	  			<desc xml:lang="fr">Catégorie, participants, contexte, langue</desc>
	  			<desc xml:lang="en">Category, participants, setting, language</desc>
	  			<element name="profileDesc"  module="header">
	  			<content>
	  				<one>
	  					<element name="textDesc"  module="corpus"/>
	  					<element name="particDesc"  module="corpus"/>
	  					<element name="settingDesc"  module="corpus"/>
	  					<element name="langUsage"  module="header"/>
	  				</one>
	  			</content> 
	  			</element>
	  			
	  			<desc xml:lang="fr">Logiciel, projet, annotations</desc>
	  			<desc xml:lang="en">Software, project, annotations</desc>
	  			<element name="encodingDesc"  module="header">
	  			<content>
	  				<zeroOrOne>
	  					<element name="appInfo"  module="header"/>
	  					<element name="projectDesc"  module="header"/>
	  					<element name="transcriptionDesc"  module="spoken"/>
	  				</zeroOrOne>
	  			</content> 
	  			</element>
		  		</sequence>
	  		</content>
	  	</elementSpec>	  	
	  	
	  	<!-- one ou oneOrMore: implique élément obligatoire, usage="req" pour les attributs obligatoires-->
	  	<!-- oneOrMore/zeroOrMore ==> bouton ajouter/modifier/supprimer dans l'interface-->
	  	<!--valList ==> choix multiples type ascenseur-->
	  	<!--desc = nom de la rubrique pour les sections, les élements à saisir, pour les valList =bulle d'aide-->
	  	<!-- value="xx" pour les attributs quand une seule valeur possible -->
	  	<!-- default="alpha num alphanum date duration url " format du champ si champ libre,  si rien pas de contrôle de format exemple titre -->
	  	
 			
	    

	    <!-- tittleStmt : un principal, de 0 à n respStmt (collecteur, transcripteur, collecteur, ...) -->	
	    <elementSpec ident="titleStmt" module="header" mode="change" default="num">     
	      <desc xml:lang="fr">Titre, Responsable(s) de la  ressource et contributeurs : collecteurs, transcripteurs, annotateurs, traducteurs, ... personnes et/ou organismes et/ou projets de recherche</desc>
	    	<desc xml:lang="en">Title, Responsible(s) of the  ressource and contributors : collectors, transcriptors, annotators, translators, ... personns and/or organisms or search projects</desc>
	      <content>
	      	<one>
	      		<elementSpec ident="title" module="core" mode="change">     
	      			<desc xml:lang="fr"> informations generales: idno = titre, bibl : la reference pour citer la ressource, desc : une courte description</desc>
	      			<desc xml:lang="en"> general information: idno = title, bibl : reference to cite this ressource, desc : a short description</desc>
	      			<content>
	      				<one>
	      				<element name="idno"  module="header">
	      					<attList>
	      						<attDef ident="type" usage="req" value="title" mode="change"/>
	      					</attList>
	      				</element>	        
	      				<element name="bibl" module="core"/>
	      				<element name="desc" module="core"/>
	      				</one>
	      			</content>  
	      		</elementSpec>
	      	</one>
		    <oneOrMore>
		    	<element name="principal"  module="header">
		    		<desc xml:lang="fr">Responsable(s) de la ressource : personne et/ou organismes et/ou projets de recherche</desc>
		    		<desc xml:lang="en">Responsible(s) of the  ressource:  personn and/or organism or search project</desc>
		    		<content>
		    			<one>
		    				<element name="name" module="core" default="alphanum">
		    				<attList>
		    					<attDef ident="type" usage="req"  mode="change"/>
		    					<valList type="closed" mode="replace">
		    						<valItem ident="person">
		    							<desc rendition="texte aide personne" xml:lang="fr">Personne</desc>
		    							<desc rendition="help text on person" xml:lang="en">Person</desc>
		    						</valItem>
		    						<valItem ident="organism">
		    							<desc rendition="texte aide laboratoire" xml:lang="fr">Laboratoire ou Institution</desc>
		    							<desc rendition="help text on search lab" xml:lang="en">Research Laboratory or Institution</desc>
		    						</valItem>
		    						<valItem ident="project">
		    							<desc rendition="texte aide projet" xml:lang="fr">Projet</desc>
		    							<desc rendition="help texte on project" xml:lang="en">Project</desc>
		    						</valItem>
		    						<valItem ident="OTHER">
		    							<desc xml:lang="fr">Autre</desc>
		    							<desc xml:lang="en">Other</desc>
		    						</valItem>
		    					</valList>
		    				</attList>		    					
		    				</element>
		    			</one>
		    		</content>  
		    	</element>
		    </oneOrMore>
	        <zeroOrMore>
	        	<element name="respStmt"  module="core">
	        		<desc xml:lang="fr">Liste des contributeurs composé d'une fonction resp et d'une liste de noms qui remplissent cette fonction </desc>
	        		<desc xml:lang="en">List of contributors made of both a role and a list of persons or oeganism who assume this role </desc>
	          	<content>
	          		<oneOrMore>
	          			<element name="resp" module="core">	          				
	          				<valList type="closed" mode="replace">
	          					<valItem ident="annotator">
	          						<desc rendition="texte aide annotateur" xml:lang="fr">annotateur</desc>
	          						<desc rendition="help text on annotator" xml:lang="en">annotator</desc>
	          					</valItem>
	          					<valItem ident="compiler">
	          						<desc rendition="texte aide coordinateur" xml:lang="fr">coordinateur</desc>
	          						<desc rendition="help text on annotator" xml:lang="en">coordinator</desc>
	          					</valItem>
	          					<valItem ident="interviewer">
	          						<desc rendition="texte aide interviewer" xml:lang="fr">interviewer</desc>
	          						<desc rendition="help text on interviewer" xml:lang="en">interviewer</desc>
	          					</valItem>
	          					<valItem ident="recorder">
	          						<desc rendition="texte aide collecteur" xml:lang="fr">collecteur</desc>
	          						<desc rendition="help text on recorder" xml:lang="en">recorder</desc>
	          					</valItem>
	          					<valItem ident="transcriber">
	          						<desc rendition="texte aide transcripteur" xml:lang="fr">transcripteur</desc>
	          						<desc rendition="help text on transcriber" xml:lang="en">transcriber</desc>
	          					</valItem>
	          					<valItem ident="translator">
	          						<desc rendition="texte aide traducteur" xml:lang="fr">traducteur</desc>
	          						<desc rendition="help text on translator" xml:lang="en">translator</desc>
	          					</valItem>
	          					<valItem ident="data_inputter">
	          						<desc rendition="texte aide normalisation" xml:lang="fr">normalisation</desc>
	          						<desc rendition="help text on data_inputter" xml:lang="en">data_inputter</desc>
	          					</valItem>	
	          					<valItem ident="OTHER">
	          						<desc xml:lang="fr">Autre</desc>
	          						<desc xml:lang="en">Other</desc>
	          					</valItem>
	          				</valList>	
	          			</element>	
	          			<element name="name" module="core" default="alphanum">
	          				<attList>
	          					<attDef ident="type" usage="req"  mode="change"/>
	          					<valList type="closed" mode="replace">
	          						<valItem ident="person">
	          							<desc rendition="texte aide personne" xml:lang="fr">Personne</desc>
	          							<desc rendition="help text on person" xml:lang="en">Person</desc>
	          						</valItem>
	          						<valItem ident="organism">
	          							<desc rendition="texte aide laboratoire" xml:lang="fr">Laboratoire ou Institution</desc>
	          							<desc rendition="help text on search lab" xml:lang="en">Research Laboratory or Institution</desc>
	          						</valItem>
	          						<valItem ident="project">
	          							<desc rendition="texte aide projet" xml:lang="fr">Projet</desc>
	          							<desc rendition="help texte on project" xml:lang="en">Project</desc>
	          						</valItem>
	          						<valItem ident="OTHER">
	          							<desc xml:lang="fr">Autre</desc>
	          							<desc xml:lang="en">Other</desc>
	          						</valItem>
	          					</valList>
	          				</attList>
	          			</element>	  				
	          		</oneOrMore>
	          	</content> 
	          </element>
	        </zeroOrMore>
	      </content>  
	    </elementSpec>

	  	<!-- publicationStmt : au moins un , un handle et une licence 
	  		par defaut Creative Common CC BY-NC-SA 2.0 FR c'est à dire "Attribution - Pas d’Utilisation Commerciale - Partage dans les Mêmes Conditions"-->	
	  	<elementSpec ident="publicationStmt" module="header" mode="change">     	  		
	  		<desc  xml:lang="fr">Information de diffusion : identifiant unique, site de réféence pour la  distribution de la ressource, liste des sites web publiant la ressource, licence</desc>
	  		<desc  xml:lang="en">Information on diffusion : handle, publisher, pubPlace, license</desc>
	  		<content>
	  			<oneOrMore>
	  				<element name="publisher" usage="req"  module="core" default="url" >
	  					<desc  xml:lang="fr">site de référence pour la  distribution de la ressource</desc>	  	
	  					<desc  xml:lang="en">main website for ressource distribution</desc>
	  				</element>
	  			</oneOrMore>
	  			<zeroOrMore>
	  				<element name="pubPlace"  module="core">
	  					<desc  xml:lang="fr">liste des sites web publiant la ressource : archive, bases de données, projets de recherche</desc>
	  					<desc  xml:lang="en">publication website of this ressource : archive , database, research project </desc>
	  					<content>
	  						<oneOrMore>
	  							<element name="ref" usage="req"/>
	  							<attList>
	  								<attDef ident="target" usage="req" mode="change" default="url">
	  									<desc  xml:lang="fr">l'uri ou l'url</desc>	  									
	  									<desc  xml:lang="en">the ressource uri or  website url</desc>
	  								</attDef>
	  							</attList>
	  						</oneOrMore>
	  					</content>
	  				</element>
	  			</zeroOrMore>
	  			<zeroOrMore>
	  				<!-- carole à vérifier avec Loïc : zero car je ne suis pas sure que chaque ressource aura un handle si elle n'est pas ds ortolang ou coccon, 
	  					more:la même ressource peut avoir plusieurs handles si déposée sur plusieurs plateformes
	  				pas de type car handle alphanum + certains caractères spéciaux ? -->
	  				<element name="idno" usage="req"  module="header"/>
	  			</zeroOrMore>
	  			<oneOrMore>
	  				<element name="availability" module="header">
	  					<attList>
	  					<!-- value=restricted car une creative common telle qu'elle soit-->
	  					<attDef ident="status" usage="req" value="restricted" mode="change"/>	  	
	  					</attList>
	  					<content>
	  						<oneOrMore>
	  							<element name="licence" module="header">
	  								<content>
	  								<one>
	  									<element name="idno" module="header"/>
	  									<attList>
	  									<attDef ident="type" usage="req"  mode="change" >
	  									<valList type="closed" mode="replace">
	  										<valItem ident="BY">
	  											<desc xml:lang="fr">Attribution</desc>
	  											<desc xml:lang="en">Attribution</desc>
	  										</valItem>
	  										<valItem ident="BY_ND">
	  											<desc xml:lang="fr">Attribution sans modification</desc>
	  											<desc xml:lang="en">Attribution and no modification</desc>
	  											</valItem>
	  										<valItem ident="BY_NC_ND">
	  											<desc xml:lang="fr">Attribution sans usage commercial et sans  modification</desc>
	  											<desc xml:lang="en">Attribution and no commercial use and no modification</desc>
	  										</valItem>
	  										<valItem ident="BY_NC">
	  											<desc xml:lang="fr">Attribution sans usage commercial</desc>
	  											<desc xml:lang="en">Attribution and no commercial use</desc>
	  										</valItem>
	  										<valItem ident="BY_NC_SA">
	  											<desc xml:lang="fr">Attribution sans usage commercial et partage suivant les mêmes modalités </desc>
	  											<desc xml:lang="en">Attribution and no Commercial use and share in the same way</desc>
	  										</valItem>
	  										<valItem ident="BY_SA">
	  											<desc xml:lang="fr">Attribution et partage suivant les mêmes modalités </desc>
	  											<desc xml:lang="en">Attribution and share in the same way</desc>
	  										</valItem>
	  										<valItem ident="OTHER">
	  											<desc xml:lang="fr">Autre</desc>
	  											<desc xml:lang="en">Other</desc>
	  										</valItem>
	  									</valList>
	  									</attDef>
	  									</attList>
	  								</one>
	  								</content>
	  							</element>						
	  						</oneOrMore>
	  					</content>
	  				</element>
	  			</oneOrMore>
	  		</content>  
	  	</elementSpec>

	  
	  	<!-- recordingStmt : recording et medias-->			
	  	<elementSpec ident="sourceDesc" module="header" mode="change"> 
	  		<desc xml:lang="fr">Enregistrement, données primaires</desc>    
	  		<desc xml:lang="en">Recording information, primary data </desc>
	  		<content>
	  			<one>
	  				<element name="recordingStmt" module="spoken">
	  					<content>
	  						<oneOrMore>
	  							<element name="recording"  module="spoken">
									<content>
									<one>
										<element name="media" usage="req"  module="spoken">
											<attList>
												<attDef ident="type" usage="req"  mode="change">
													<valList type="closed">
														<valItem ident="audio"/>
														<valItem ident="video"/>
														<valItem ident="other"/>
													</valList>
												</attDef>
												<attDef ident="mimeType" usage="req"  mode="change">
													<valList type="closed">
														<valItem ident="audio/wav"/>
														<valItem ident="audio/mp3"/>
														<valItem ident="video/mp4"/>
														<valItem ident="video/mov"/>
														<valItem ident="video/wmv"/>
														<valItem ident="video/avi"/>
														<valItem ident="other"/>
													</valList>
												</attDef>
												<attDef ident="dur-iso" usage="req"  mode="change" default="duration">
													<desc xml:lang="fr">durée du média : chaque medias peut avoir une durée différente</desc>
													<desc xml:lang="en">media duration : each media could have a different duration </desc>
												</attDef>
												<attDef ident="url" mode="change" default="url"/>
											</attList>
											<content>
												<one>
													<element name="desc">
														<attList>
														<attDef ident="type" usage="req" value="quality" mode="change"/>
														<attDef ident="subtype" usage="req"  mode="change">
															<valList>
																<valItem ident="less_noisy">
																	<desc xml:lang="fr">inaudible ou bruité moins de 5%</desc>
																	<desc xml:lang="en">inaudible or noisy less than 5%</desc>
																</valItem>
																<valItem ident="noisy">
																	<desc xml:lang="fr">inaudible ou bruité plus de 5%</desc>
																	<desc xml:lang="en">inaudible or noisy more than 5%</desc>
																</valItem>
																<valItem ident="soundproof_room">
																	<desc xml:lang="fr">chambre sourde</desc>
																	<desc xml:lang="en">soundproof</desc>
																</valItem>	
																<valItem ident="OTHER">
																	<desc xml:lang="fr">Autre</desc>
																	<desc xml:lang="en">Other</desc>
																</valItem>
															</valList>
														</attDef>	
														</attList>
													</element>	   
												</one>
												<one>
													<element name="desc">
														<attList>
															<attDef ident="type" usage="req" value="anonymization" mode="change"/>
															<attDef ident="subtype" usage="req"  mode="change">
																<valList>
																	<valItem ident="anonymization">
																		<desc xml:lang="fr">anonymisée</desc>
																		<desc xml:lang="en">anonymized</desc>
																	</valItem>
																	<valItem ident="no_anonymization">
																		<desc xml:lang="fr">non anonymisée</desc>
																		<desc xml:lang="en">no anonymization</desc>
																	</valItem>
																	<valItem ident="partially_anonymized">
																		<desc xml:lang="fr">anonymisation partielle</desc>
																		<desc xml:lang="en">partially anonymized</desc>
																	</valItem>
																	<valItem ident="OTHER">
																		<desc xml:lang="fr">Autre</desc>
																		<desc xml:lang="en">Other</desc>
																	</valItem>
																</valList>
															</attDef>
														</attList>
													</element>	   
												</one>
											</content>
									</element>
									</one>
									</content>	        
	  							</element>
	  						</oneOrMore>
	  					</content>
	  				</element>
	  			</one>
	  	  </content>
	  	</elementSpec>

	  	<!-- textDesc : catégorisation du document oral-->
	  	<!-- attention il y a aussi un module additionnel setting ==> détail de la situation, artefact, plan,   ...-->
	  	<elementSpec ident="textDesc" module="corpus" mode="change"> 
	  		<desc xml:lang="fr">Quel type de corpus oraus, un autre module Situation/setting commplètera cette partie (artefact, plan, ...)</desc>    
	  		<desc xml:lang="en">Which kind of oral corpora,an additonal setting element will complete description (artefact, map, ...)</desc>
	  		<content>
	  			<one>
	  				
	  				<element name="channel" module="corpus">
	  					<attList>
	  						<attDef ident="mode" usage="req" value="s" /> <!-- mode="s"= default value qui signifie spoken -->
	  						<attDef ident="subtype" usage="req"  mode="change">
	  							<valList>
	  								<valItem ident="face_to_face">
	  									<desc xml:lang="fr">tous les locuteurs sont présents</desc>
	  									<desc xml:lang="en">all speakers are in the same setting</desc>
	  								</valItem>
	  								<valItem ident="radio">
	  									<desc xml:lang="fr">à la radio</desc>
	  									<desc xml:lang="en">on radio</desc>
	  								</valItem>
	  								<valItem ident="tv">
	  									<desc xml:lang="fr">à la télévisison</desc>
	  									<desc xml:lang="en">on tv</desc>
	  								</valItem>
	  								<valItem ident="telephone">
	  									<desc xml:lang="fr">au téléphone</desc>
	  									<desc xml:lang="en">over the telephone</desc>
	  								</valItem>
	  								<valItem ident="visioconference">
	  									<desc xml:lang="fr">en visioconference</desc>
	  									<desc xml:lang="en">visioconference</desc>
	  								</valItem>
	  							</valList>
	  						</attDef>
	  					</attList>
	  				</element>
  				
	  				<element name="constitution" module="corpus">
	  					<attList>
	  						<attDef ident="type" usage="req"  mode="change" value="single"> 
	  							<valList>
	  								<valItem ident="single">
	  									<desc xml:lang="fr">autonome</desc>
	  									<desc xml:lang="en">autonomous</desc>
	  								</valItem>
	  								<valItem ident="composite">
