				<!-- settingDesc : recording et medias-->
				<elementSpec ident="settingDesc" module="corpus" mode="change">
					<desc xml:lang="fr"> lieu, langue et date d'enregistrement obligatoire au moins
						pour la langue de l'enregistrement et le pays cf Module additionnel contexte
						et sociliolinguistique (Informe sur le lieu de recueil. S'il y en a
						plusieurs, il faut répéter l'élément "place") </desc>
					<desc xml:lang="en"> Place, language and date are mandatory at least recording
						language and country cf additional module context and sociolinguistic give
						informations on setting place, maybe several place elements </desc>

					<content>
						<sequence minOccurs="1" maxOccurs="1">
							<elementRef key="listPlace"/>
							<elementRef key="setting"/>
							<!-- langUsage : langue de l'enregistrement, plusieurs possibles avec % d'utilisation-->
							<elementRef key="langUsage"/>
						</sequence>
					</content>
				</elementSpec>

				<elementSpec ident="listPlace" module="namesdates">
					<content>
						<elementRef key="place" minOccurs="1" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="place" module="namesdates">
					<content>
						<elementRef key="country" minOccurs="1" maxOccurs="1"/>
						<elementRef key="settlement" minOccurs="0" maxOccurs="unbounded"/>
						<elementRef key="district" minOccurs="0" maxOccurs="unbounded"/>
						<!-- TDOD : plusieurs desc ??? desc:  piece, exemple changement de lieu avec micro cravate -->
						<elementRef key="desc" minOccurs="0" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="country" module="namesdates">
					<attList>
						<!-- attribut ref= norme iso ISO 3366-1 => Indiquer le pays de recueil , donnée minimale en général connue, fr par défaut -->
						<attDef ident="name" usage="req" mode="change" rend="fr"/>
					</attList>
				</elementSpec>

				<elementSpec ident="district" module="namesdates">
					<attList>
						<attDef ident="name" usage="rec" mode="change" rend="fr"/>
					</attList>
				</elementSpec>

				<elementSpec ident="settlement" module="namesdates">
					<attList>
						<!-- ref= norme TGN ? Indiquer la ville, village de recueil : référentiel Getty Thesaurus of Geographic Name / code UNECE des Nations Unies plus fin que le code postal-->
						<attDef ident="ref" usage="req" mode="change" rend="fr"/>
					</attList>
				</elementSpec>

				<elementSpec ident="setting" module="corpus">
					<content>
						<elementRef key="date" minOccurs="0" maxOccurs="unbounded"/>
						<!-- description de la situation ==> attention aux redondances ==> description déjà dans title -->
						<elementRef key="activity" minOccurs="0" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="activity" module="namesdates">
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="p" module="core">
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="date" module="core">
					<attList>
						<!-- date : soit intervalle notBefore-iso/notAfter-iso, soit date exacte avec l'attribut when-iso
	  							     ds le modèle normalement contrainte soit when soit before/after-->
						<attDef ident="notBefore-iso" usage="opt" mode="change"/>
						<attDef ident="notAfter-iso" usage="opt" mode="change"/>
						<attDef ident="when-iso" usage="opt" mode="change"/>
					</attList>
				</elementSpec>

				<!-- langUsage : langue de l'enregistrement, plusieurs possibles avec % d'utilisation-->
				<elementSpec ident="langUsage" module="header" mode="change">
					<desc xml:lang="fr">Langue(s) de l'enregistrement, attribut usage avec le %
						d'utilisation si plusieurs langues</desc>
					<desc xml:lang="en">Recording language(s), use attribute usage with the
						percentage if several languages</desc>
					<content>
						<elementRef key="language" minOccurs="1" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="language" module="header">
					<attList>
						<!-- code iso RFC 3066"-->
						<attDef ident="usage" usage="req" mode="change" rend="100"/>
						<attDef ident="ident" usage="req" mode="change" rend="fr"/>
					</attList>
				</elementSpec>

				<elementSpec ident="appInfo" module="header" mode="change">
					<desc xml:lang="fr"> TEI_CORPO + release ou autre "convertisseur maison" (C'est
						ici qu'on indique la référence à l'application ayant généré le fichier TEI)
						Description obligatoire en TEI donc générique de l'application , evt options
						de traitement pour elan, praat, choix des locuteurs, ... </desc>
					<desc xml:lang="en">Which kind of oral corpora,an additonal setting element will
						complete description (artefact, map, ...)</desc>
					<content>
						<sequence minOccurs="0" maxOccurs="unbounded">
							<elementRef key="application"/>
							<!-- desc:  complément sur les options/choix/logiciels "maison"-->
							<elementRef key="desc"/>
						</sequence>
					</content>
