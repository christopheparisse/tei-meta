{
  "name": "teimeta",
  "productName": "TeiMeta",
  "version": "0.5.6",
  "description": "An editor for TEI CORPO metadata",
  "main": "index.js",
  "scripts": {
    "start": "electron index.js",
    "run": "electron temp-electron/index.js",
    "page": "gulp page; webpack --mode=none",
    "electron": "gulp electron; cd temp-electron ; npm install",
    "readme": "marky-markdown README.md > readme.html",
    "webpack": "webpack --mode=none",
    "iso639": "awk -f iso639.awk models/ISO-639-2_utf-8.txt | grep -v \"|\" > teiedit/iso639.js"
  },
  "keywords": [
    "Electron",
    "tei",
    "TEICORPO",
    "metadata"
  ],
  "author": "Christophe Parisse",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/christopheparisse/teimeta/issues"
  },
  "homepage": "https://github.com/christopheparisse/teimeta#readme",
  "devDependencies": {
    "@types/jquery": "^2.0.49",
    "@types/node": "^6.0.113",
    "awesome-typescript-loader": "^3.5.0",
    "css-loader": "^0.27.3",
    "electron": "^2.0.3",
    "electron-builder": "^20.15.1",
    "electron-packager": "^12.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "gulp": "^3.9.1",
    "gulp-concat": "latest",
    "gulp-html-tag-include": "^1.0.1",
    "gulp-preprocess": "latest",
    "gulp-rename": "^1.3.0",
    "gulp-typescript": "^3.2.4",
    "marky-markdown": "^12.0.0",
    "run-sequence": "^1.2.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.16.1",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "entities": "^1.1.1",
    "file-saver": "^1.3.8",
    "fs": "0.0.1-security",
    "picomodal": "^3.0.0",
    "rome": "^2.1.22",
    "xpath": "0.0.23"
  }
}
