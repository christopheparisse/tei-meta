        "./temp-electron/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "temp-electron/node_modules"
    ],
    "compilerOptions": {
        "noImplicitAny": false,
        "allowJs": false,
        "target": "es5",
        "module": "commonjs",
